<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <h1>User Profile</h1><hr>
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><strong><h3>My Orders</h3></strong></h3>
            </div>
        {{# each orders}}
            <div class="panel panel-default">
                <div class="panel-body">
                    <ul class="list-group">
                        {{# each this.items}}
                            <li class="list-group-item">
                                <span class="badge">{{this.qty}}Units</span>{{this.item.title}} , Rs.{{this.price}}
                            </li>
                        {{/ each}}
                    </ul>
                </div>
            <div class="panel-footer"><strong>Total Price:Rs.{{this.cart.totalPrice}}</strong></div>
            </div>
        {{/ each}}
    </div>
</div>